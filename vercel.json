{
  "version": 2,
    "name": "backendekyc",

  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "crons": [
    {
      "path": "/internal/cron/seo-regenerate",
      "schedule": "0 2 * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
  
}
